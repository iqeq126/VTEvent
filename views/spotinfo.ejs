                            size: 'sm',
                            beforeDateTime: '<%= raw_end_date %>'
                        });
                    }
                    else{
                        jQuery('#remain-time').flipcountdown({
                            size: 'xs',
                            beforeDateTime: '<%= raw_end_date %>'
                        });
                    }
                }
