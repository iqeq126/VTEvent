                        jQuery('#remain-time2').flipcountdown({
                            size: 'sm',
                            beforeDateTime: '<%= start_date %>'
                        });
                    }
                    else{
                        jQuery('#remain-time2').flipcountdown({
                            size: 'xs',
                            beforeDateTime: '<%= start_date %>'
                        });
                    }
