    <script>
        function visitCheck(){
            return '<%=is_exists_user_info === 1 ? ( already_visit > 0 ? 1: 0) : 0%>' === '1';
        }
        function goodsCheck(){
            return parseInt('<%=is_exists_user_info === 1 ? (already_assigned_goods > 0 ? already_assigned_goods : 0): 0%>');
        }
        function duplicateCheck(){
            return parseInt('<%=duplicate_goods_count%>') - goodsCheck();
        }
        function totalCheck(){
            return visitCheck() && (duplicateCheck() >= 0);
        }
        /*
        function Visits(){
            let IsVisit = document.getElementById("visit_confirm");
            if(visitCheck())
                IsVisit.innerHTML = "방문 여부 : O";
            else {
                IsVisit.innerHTML = "방문 여부 : X";
            }
        }*/
        function Goods(){
            // 이미 방문하고 상품을 할당받았는가
            let IsGoods = document.getElementById("goods_confirm");
            if(duplicateCheck() > 1){
                if((goodsCheck() > 0) && visitCheck()) {
                    swapDiv();
                }
                else {
                    IsGoods.innerHTML = "이곳에서 상품을 수령하지 않았습니다.\n";
                }
            }
        }

        // 상품 수령 개수
        function duplication_confirm(){
            let myId = document.getElementById('goodsId');
            if(<%=is_exists_user_info%> === 0){
                myId.innerHTML = '사용자 정보가 없습니다.';
            }
            else if (duplicateCheck() >= 1){
                myId.innerHTML = '앞으로 <b style="color:#E74C3C; font-weight: 300;">'+ duplicateCheck() + '번 더</b> 받을 수 있습니다.';
            }
            else{
                myId.innerHTML = '더 이상 상품을 받을 수 없습니다.';
                /*
                let disabledSubmit =  document.getElementById('gameAlert');
                disabledSubmit.style.background = 'gray';
                disabledSubmit.disabled = 'disabled';
                 */
            }
        }

        let t = document.getElementById('deadline');

        $(function(){
            const remainTime = document.querySelector("#remain-time");
            const myId = document.getElementById('goodsId');
            let gConfirm = document.getElementById('goods_confirm');
            let vConfirm = document.getElementById('visit_confirm');
            function diffDay() {
                const masTime = new Date('<%=raw_end_date%>');
                const tTime = new Date('<%=raw_start_date%>');
                const todayTime = new Date();

                if (todayTime > tTime && todayTime < masTime) {
                    $('#remain-time2').hide();
                    if(remainTime.innerText === '이벤트는 2월 9일 9시에 시작됩니다.')
                        remainTime.innerText = '';
                    if (matchMedia("screen and (min-width: 1440px)").matches) {
